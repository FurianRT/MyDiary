<resources>

    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar.Bridge">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="actionModeBackground">@color/colorPrimaryDark</item>
        <item name="windowActionModeOverlay">true</item>

        <item name="autoCompleteTextViewStyle">@style/SearchAutoCompleteTextView</item>
        <item name="android:textAlignment">gravity</item>
        <item name="android:textDirection">locale</item>
        <item name="android:windowBackground">@drawable/splash_screen</item>
    </style>

    <style name="AppTheme.TransparentActionBar">
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@color/transparent_black</item>
    </style>

    <style name="SearchAutoCompleteTextView" parent="Widget.AppCompat.Light.AutoCompleteTextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">#99FFFFFF</item>
    </style>

    <style name="AppTheme.Toolbar" parent="ThemeOverlay.MaterialComponents.ActionBar">
        <item name="popupTheme">@style/ThemeOverlay.MaterialComponents.Light</item>
        <item name="theme">@style/ThemeOverlay.MaterialComponents.Dark.ActionBar</item>
    </style>

    <style name="TextAppearance.App.TextInputLayout" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">11sp</item>
    </style>

